	</item>
	<item>
		<id>2</id>
		<name>会员用户</name>
		<key>member</key>
		<config></config>
		<style><![CDATA[<font style="color:#0033CC;">{$name}</font>]]></style>
	</item>
	<item>
		<id>5</id>
		<name>VIP用户</name>
		<key>vip</key>
		<config></config>
		<style><![CDATA[<font style="color:#990099;"><b>{$name}</b></font>]]></style>
	</item>
</xcml>