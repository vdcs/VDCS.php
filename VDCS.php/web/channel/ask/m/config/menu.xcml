<?xml version="1.0" encoding="utf-8"?>
<xcml version="1.0" model="data">
	<configure>
		<node>item</node>
		<field>name,url,config</field>
		<title><![CDATA[]]></title>
		<relate></relate>
	</configure>
	<item>
		<name><![CDATA[{@question:names}列表]]></name>
		<url><![CDATA[params=portal=question&action=list]]></url>
		<config><![CDATA[icon=list;act=action.list]]></config>
	</item>
	<item>
		<name><![CDATA[sup.添加]]></name>
		<url><![CDATA[p=portal;params=action=add]]></url>
		<config><![CDATA[icon=;act=action.add]]></config>
	</item>
	<item>
		<name><![CDATA[{@name}分类]]></name>
		<url><![CDATA[channel=system;p=model;m=class;params=subchannel={@channel}]]></url>
		<config><![CDATA[icon=class;act=model.class]]></config>
	</item>
	<item>
		<name><![CDATA[频道配置]]></name>
		<url><![CDATA[channel=system;p=model;m=configure;params=subchannel={@channel}]]></url>
		<config><![CDATA[icon=configure;act=model.configure]]></config>
	</item>
	
	<item>
		<name><![CDATA[sub.{@question:names}解决]]></name>
	</item>
	<item>
		<name><![CDATA[已解决{@question:names}]]></name>
		<url><![CDATA[params=portal=question&mode=answer]]></url>
		<config><![CDATA[icon=point;act=action.list]]></config>
	</item>
	<item>
		<name><![CDATA[未解决{@question:names}]]></name>
		<url><![CDATA[params=portal=question&mode=question]]></url>
		<config><![CDATA[icon=point;act=action.list]]></config>
	</item>
	
	<item>
		<name><![CDATA[sub.{@question:names}状态]]></name>
	</item>
	<item>
		<name><![CDATA[已推荐{@question:names}]]></name>
		<url><![CDATA[params=portal=question&mode=gooda]]></url>
		<config><![CDATA[icon=point;act=action.list]]></config>
	</item>
	<item>
		<name><![CDATA[已固顶{@question:names}]]></name>
		<url><![CDATA[params=portal=question&mode=topa]]></url>
		<config><![CDATA[icon=point;act=action.list]]></config>
	</item>
</xcml>