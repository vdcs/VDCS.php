	<var>
		<title>Passport</title>
		<title.index>null</title.index>
		
		<title.login>登录</title.login>
		<title.logout>退出</title.logout>
		<title.register>注册</title.register>
		<title.activation>激活帐户</title.activation>
		<title.invite>注册邀请</title.invite>
		<title.agreement>服务协议</title.agreement>
		
