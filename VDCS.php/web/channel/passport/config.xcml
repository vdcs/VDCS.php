		<name.max>50</name.max>
		<password.min>4</password.min>
		<password.max>20</password.max>
		<email.min>5</email.min>
		<email.max>100</email.max>
		<names.min>2</names.min>
		<names.max>50</names.max>
		<realname.min>4</realname.min>
		<realname.max>20</realname.max>
		<pass5.is>1</pass5.is>
	</num>
	<config>
		<url>default</url>
		<name.hold>guest,游客</name.hold>
		<name.holds>admin</name.holds>
