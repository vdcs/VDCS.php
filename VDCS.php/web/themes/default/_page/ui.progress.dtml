                                                        <div class="controls">
                                                            <div class="progress progress-striped">
                                                                <div class="bar" style="width: 31%; "></div>
                                                            </div>
                                                            <span class="help-block"><code>progress progress-striped</code></span>
                                                        </div>
                                                    </div>
                                                    <div class="control-group">
                                                        <label class="control-label">
                                                            Active Progress Bar
                                                        </label>
