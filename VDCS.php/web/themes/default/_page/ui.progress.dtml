<div class="framer">

<div id="progress">
                                        <div class="widget">
                                            <div class="widget-header">
                                                <span class="title"><i class="icon-progress-bar"></i> Progress Bar Types</span>
                                            </div>
                                            <div class="widget-content form-container">
                                                
                                                <form class="form-horizontal">
                                                    <div class="control-group">
                                                        <label class="control-label">
                                                            Usual Progress Bar
                                                        </label>
                                                        <div class="controls">
                                                            <div class="progress">
                                                                <div class="bar" style="width: 67%; "></div>
                                                            </div>
                                                            <span class="help-block"><code>progress</code></span>
                                                        </div>
                                                    </div>
                                                    <div class="control-group">
                                                        <label class="control-label">
                                                            Progress Bar With Label
                                                        </label>
                                                        <div class="controls">
                                                            <div class="progress">
