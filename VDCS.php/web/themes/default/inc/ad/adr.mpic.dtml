<label:include script="<url:script>player/pics.js">
<div id="adr-mpic" class="ac" style="width:480px;height:300px;"></div>
<script type="text/javascript">
$d.load(function(){
	var pics=new VDCS.Player.Pics();
	pics.setStyle("hot");
